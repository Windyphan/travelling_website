{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLOUDFLARE_API_TOKEN": "@cloudflare-api-token",
    "CLOUDFLARE_ACCOUNT_ID": "@cloudflare-account-id",
    "CLOUDFLARE_DATABASE_ID": "@cloudflare-database-id"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}
